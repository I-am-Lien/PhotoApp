<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".ui.edit_image.edit_image_parameters.ConstrastFragment">


    <SeekBar
        android:id="@+id/sb_constrast"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin_horizontal_seekbar"
        android:max="20"
        android:progress="0"
        android:progressDrawable="@drawable/seekbar_backgroud"
        android:thumb="@drawable/seekbar_thumb"/>

</LinearLayout>
</layout>